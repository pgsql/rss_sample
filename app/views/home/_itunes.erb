      <% if data && data["title"] %>
        Title: <%= data["title"] %> <br/>
      <% end  %>
      <% content1 = data["enclosure"]%> 
      <% if data && data["author"] %>
        Author: <%= data["author"] %> <br/>
      <% end  %>

      <% if data && data["guid"] %>
      GUID: <%= data["guid"] %> <br/>
      <% end  %>

      <% if data && data["link"] %>
        Link: <%= data["link"] %> <br/>
      <% end  %>


      <% if data && data["enclosure"] %>
        URL:  <%= data["enclosure"]["url"] unless data["enclosure"].blank?  %><br/>
      <% end %>

      <% if data && data["description"] %>
        Description: <%= data["description"] %> <br/>
      <% end  %>
      <% if data && data["keywords"] %>
        Keywords: <%= data["keywords"] %> <br/>
      <% end  %>

      <% if content1.class.to_s == "Hash" %>
        Content URL:<%= content1["url"] %> <br/>
          <% if content1 && content1["keywords"] %>
            Content Keywords: <%= content1["keywords"] %> <br/>
          <% end %>
          <% if content1 && content1["tags"] %>
            Content Tags: <%= content1["tags"] %> <br/>
          <% end %>
      <% end  %>
      <% if content1.class.to_s == "Array" %>
         Content URL:<%= content1.first["url"]  %> <br/>
      <% end  %>
